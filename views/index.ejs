<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" type="text/css" href="/css/style.css" />
    <title>songwriter</title>
  </head>
  <body>
    <h1>songwriter - Chords, Lyrics, Notation, Tabber</h1>
    <div id="songs-container">
      <% for (let i = 0; i < songs.length; i ++) { %> <% if (songs[i].request
      === false) { %>
      <div id="song">
        <a href="/<%=songs[i]._id%>">
          <img src="<%=songs[i].img%>" width="90px" height="90px" />
        </a>
        <ul>
          <li id="song-name"><%=songs[i].artist%> - <%=songs[i].title%></li>
          <li>tabbed by <%=songs[i].author%></li>
        </ul>
      </div>
      <% } %> <% } %>
    </div>
    <a href="/new">contribute my own tablature</a>
    <a href="/requests">requests</a>
  </body>
</html>
